@model ApplicationCore.Models.CastDetailsResponseModel
<div class="container">
<div>
<img src="@Model.ProfilePath"/>
</div>
<div>
@Model.Name
</div>
<div>
@Model.Gender
</div>
<div>
@Model.TmdbUrl
</div>
<div class="container-fluid">
<div class ="row">
    @foreach(var movie in Model.MovieTitle)
{
    <div class="col-6 col-xl-2 cl-sm-4 col-lg-3">
    <partial name="_MovieCard" model="movie"/>
    </div>
}

</div>
</div>


</div>